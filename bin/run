#! /bin/bash
cd `dirname $0`/..

while [ $# -gt 0 ]
do
    case "$1" in
        -X*)
            java_opts="${java_opts} $1"
            ;;
        -D*)
            java_opts="${java_opts} $1"
            ;;
        *)
            clojure_args="${clojure_args} $1"
            ;;
    esac
    shift
done

/usr/bin/env java -server -cp `bin/classpath` clojure.main  ${clojure_args}
